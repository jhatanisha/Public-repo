class Solution:
    def mostCommonWord(self, paragraph, banned):
        banned = set(banned)
        
        for ch in "!?',;.":
            paragraph = paragraph.replace(ch, " ")
        
        words = paragraph.lower().split()
        
        freq = {}
        for w in words:
            if w not in banned:
                freq[w] = freq.get(w, 0) + 1
        
        return max(freq, key=freq.get)
